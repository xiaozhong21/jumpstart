{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist",
    "sourceMap": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "node_modules/",
      ],
      "typings/*": [
        "src/typings/*"
      ]
    },
  },
  "include": [
    "src"
  ]
}
